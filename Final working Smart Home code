int val;
int sensor=A1;
int readval;
int del=500;
int red=7;
float mv;
float cel;

void setup() {
  // put your setup code here, to run once:
  pinMode(sensor,INPUT);
  pinMode(red,OUTPUT);
  
Serial.begin(9600); 
}

void loop() {
  // put your main code here, to run repeatedly:
val=analogRead(sensor);
mv=(val/1024.0)*5000;
cel=mv/10;
Serial.println(val);
delay(del);

if (val>51){
  digitalWrite(red,HIGH);
}

if (val<51){
  digitalWrite(red,LOW);
}
}









//LCM 1602
